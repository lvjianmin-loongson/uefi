#string STR_GEN_TOO_MANY            #language en-US "%H%s%N: Too many arguments for flag - '%H%s%N'\r\n"
#string STR_GEN_NO_VALUE            #language en-US "%H%s%N: Missing argument for flag - '%H%s%N'\r\n"
